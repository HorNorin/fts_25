<div class="col-sm-8 col-sm-offset-2">
  <div class="row">
    <div class="col-sm-6">
      <h2>All Question</h2>
    </div>
    <div class="col-sm-6">
      <br />
      <%= link_to t(:label, label: "New Question"), new_admins_question_path, class: "btn btn-primary pull-right" %>
    </div>
  </div>
  <%= will_paginate @questions, renderer: BootstrapPagination::Rails %>
  <ul class="list-group">
    <%= render @questions %>
  </ul>
</div>
